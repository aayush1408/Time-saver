{
    "manifest_version":2,
    "name":"Value time",
    "version":"0.001",
    "content_scripts":[
        {
            "matches":[
                "*://github.com/*",
                "*://youtube.com/",
                "*://twitter.com/*",
                "*://facebook.com/*"
            ],
            "js":["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"Stopwatch.png"
    }
}
